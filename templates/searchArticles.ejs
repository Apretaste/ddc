<div class="row">
    <div class="col s12">
        <h1>Buscar:
    	    <%= search %>
	</h1>
    </div>
</div>

<% articles.forEach(function(article) { %>
<div class="row">
    <div class="col s12">
	<small>
	    <font color = "gray">
		<%= formatDate(article.pubDate) %>
	    </font>
	</small>
	<br/>
	<b><a href = "#!" onclick = "apretaste.send({command:'DIARIODECUBA HISTORIA', data:{link: article.link}})"><%= article.title %></a></b>
	<%= article.description %>
	<br/>
    </div>
</div>
<% }); %>
</div>

<div class="row">
    <div class="col s12">
	<button class = "btn waves-effect waves-light" onclick = "apretaste.send({command:'DIARIODECUBA'})">
	    Titulares
	    <i class = "material-icons right">feed</i>
	</button>
    </div>
</div>

